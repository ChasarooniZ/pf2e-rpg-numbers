    "description": "A module that adds a lot neat rpg and anime themed effects to pf2e.",
    "version": "#{VERSION}#",
    "compatibility": {
        "minimum": "13",
        "verified": "13"
    },
    "authors": [
        {
            "name": "chasarooniZ"
        }
    ],
