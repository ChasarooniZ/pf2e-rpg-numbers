          "path": "languages/fr.json"
        },
        {
          "lang": "de",
          "name": "Deutsch",
          "path": "languages/de.json"
        },
        {
          "lang": "pl",
          "name": "Polish",
          "path": "languages/pl.json"
        },
        {
          "lang": "ja",
          "name": "日本語 (Japanese)",
          "path": "languages/ja.json"
        },
        {
          "lang": "ko",
          "name": "한국어 (Korean)",
          "path": "languages/ko.json"
        },
        {
          "lang": "ru",
          "name": "Russian",
          "path": "languages/ru.json"
        }
    ],
    "packs": [
      {
        "name": "pf2e-rpg-numbers-macros",
